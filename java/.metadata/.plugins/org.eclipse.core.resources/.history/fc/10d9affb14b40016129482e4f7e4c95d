package br.com.empresa;

import java.util.GregorianCalendar;

import br.com.empresa.model.Funcionario;
import br.com.empresa.model.Gasto;

public class Principal {
	public static void main(String[] args) {
		Funcionario funcionario = new Funcionario("Jose", 23, new GregorianCalendar(1993, 12, 15));
		System.out.println(funcionario);
		
		new Gasto("Comida", 45, funcionario, new GregorianCalendar().getInstance());
	}
}
