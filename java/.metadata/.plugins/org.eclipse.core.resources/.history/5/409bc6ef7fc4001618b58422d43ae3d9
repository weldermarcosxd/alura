package br.com.alura.model;

public class Produto {
	
	private Integer id;
	private String nome;
	private String descricao;
	
	public Produto(Integer id, String nome, String descricao) {
		this.id = id;
		this.nome = nome;
		this.descricao = descricao;
	}
	
	public Produto(Integer id, String nome, String descricao) {
		this.nome = nome;
		this.descricao = descricao;
	}
	
	public Produto() {
		// TODO Auto-generated constructor stub
	}
	
	

}
