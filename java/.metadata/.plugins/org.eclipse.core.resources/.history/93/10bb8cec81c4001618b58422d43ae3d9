public class TestProdutoSave {

	private ProdutoDAO produtoDAO = new ProdutoDAO();
	
	public static void main(String[] args) {
		Produto produto = new Produto("Mesa Azul", "4 pernas");
		
		produtoDAO.save(produto);
	}
}
